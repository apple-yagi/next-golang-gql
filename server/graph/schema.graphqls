scalar Time

type Todo {
  id: ID!
  text: String!
  done: Boolean!
  createdAt: Time!
  updatedAt: Time!
  user: User!
}

type User {
  id: ID!
  name: String!
}

type Query {
  todos: [Todo!]!
}

input NewTodo {
  text: String!
  userId: String!
}

type Mutation {
  createTodo(input: NewTodo!): Todo!
}
